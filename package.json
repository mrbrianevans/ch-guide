{
  "name": "sveltepress",
  "version": "1.0.0",
  "scripts": {
    "dev": "node scripts/generateProdData.js && svelte-kit dev -p 5002",
    "build": "node scripts/generateProdData.js && svelte-kit build",
    "start": "node build",
    "preview": "svelte-kit preview",
    "postinstall": "ni svelte -D --ignore-scripts && node scripts/generateSymlinks.js",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "preinstall": "node scripts/prepareTheme.js",
    "sitemap": "node scripts/generateSitemap.js"
  },
  "dependencies": {
    "@antfu/ni": "0.14.0",
    "@carbon/themes": "10.54.0",
    "@sveltejs/adapter-netlify": "1.0.0-next.51",
    "@sveltejs/adapter-node": "1.0.0-next.73",
    "@sveltejs/adapter-static": "1.0.0-next.29",
    "@sveltejs/adapter-vercel": "1.0.0-next.47",
    "@sveltejs/kit": "1.0.0-next.306",
    "carbon-components": "10.56.0",
    "carbon-components-svelte": "0.62.3",
    "carbon-icons-svelte": "10.45.1",
    "carbon-preprocess-svelte": "0.7.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "3.4.1",
    "flexsearch": "0.7.21",
    "front-matter": "4.0.2",
    "highlight.js": "11.5.0",
    "isomorphic-git": "^1.17.1",
    "jsesc": "3.0.2",
    "marked": "4.0.12",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.6.0",
    "sanitize-html": "2.7.0",
    "sass": "1.49.11",
    "svelte-preprocess": "4.10.5",
    "svelte-typewriter": "2.4.5",
    "xml": "^1.0.1"
  },
  "type": "module",
  "license": "Unlicense",
  "devDependencies": {
    "@carbon/themes": "10.54.0",
    "carbon-components": "10.56.0",
    "carbon-components-svelte": "0.62.3",
    "carbon-icons-svelte": "10.45.1",
    "carbon-preprocess-svelte": "0.7.0",
    "svelte": "^3.46.6",
    "svelte-typewriter": "2.4.5"
  }
}
